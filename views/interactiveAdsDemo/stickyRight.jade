//- Interactive ads demo view

extends ../layout/base

block getTitle
    - var titleVar = 'AdPushup Interactive Ads Sticky Top Demo'

block meta
    meta(content="width=device-width,initial-scale=1" name="viewport")
    
block headCss
    link(href="http://fonts.googleapis.com/css?family=Karla:400,700,400italic,700italic&subset=latin,latin-ext", rel='stylesheet', type='text/css')
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
    style.
        .sticky-ad-right {
            position: fixed;
            right: 0;
            bottom: 0; 
            margin-left: auto;
            margin-right: auto;
            width: 160px;
            height: 600px;
            background: #3ed6f9;
            z-index: 1000;
            color: #333;
            font-size: 1.8em;
            font-weight: 300;
            text-align: center;
            padding: 25px;
        }
        .jumbotron {
            border-radius: 6px;
        }
        @media screen and (max-width: 940px) {
            .desktop {
                display: none;
            }
        }

