//- Interactive ads demo view

extends ../layout/base

block getTitle
    - var titleVar = 'AdPushup Interactive Ads Sticky Top Demo'

block meta
    meta(content="width=device-width,initial-scale=1" name="viewport")

block headCss
    link(href="http://fonts.googleapis.com/css?family=Karla:400,700,400italic,700italic&subset=latin,latin-ext", rel='stylesheet', type='text/css')
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
    style.
        .sticky-ad-bottom {
            position: fixed;
            bottom: 0;
            left: 0;
            right: 0;
            margin-left: auto;
            margin-right: auto;
            width: 728px;
            height: 90px;
            background: #3ed6f9;
            z-index: 1000;
            color: #333;
            font-size: 1.8em;
            font-weight: 300;
            text-align: center;
            padding: 25px;
        }
        .mobile { 
            display: none;
        }
        .jumbotron {
            padding-top: 90px;
        }
        footer {
            margin-bottom: 90px;
        }
        @media screen and (max-width: 767px) {
            .desktop {
                display: none;
            }
            .mobile {
                display: block;
                width: 300px;
                height: 50px;
                padding: 7px;
            }
            .jumbotron {
                padding-top: 50px;
            }
            footer {
            margin-bottom: 50px;
        }
        }

