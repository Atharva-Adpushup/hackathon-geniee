//- Interactive ads demo view

extends ../layout/base

block getTitle
    - var titleVar = 'AdPushup Interactive Ads Native Demo'

block meta
    meta(content="width=device-width,initial-scale=1" name="viewport")
    
block headCss
    link(href="http://fonts.googleapis.com/css?family=Karla:400,700,400italic,700italic&subset=latin,latin-ext", rel='stylesheet', type='text/css')
    link(rel='stylesheet' href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css")
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
    style.
        .native-ad {
            padding: 0px 10px 10px;
            border: 1px solid #eeeeee;
        }
        .img {
            padding: 10px;
            font-size: 2em;
            text-align: center;
            color: #333;
            background: #eeeeee;
            margin-left: -10px;
            margin-right: -10px;
        }
        .ad-label {
            color: #fff;
            background: #23bb3c;
            padding: 5px;
            display: inline-block;
            margin-top: 10px;
            font-size: .8em;
            border-radius: 3px;
        }
        .ad-label + p {
                display: inline;
                margin-left: 5px;
        }

